{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "BACKEND_URL": "https://your-railway-app.railway.app",
    "NEXT_PUBLIC_API_URL": "https://your-railway-app.railway.app"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-railway-app.railway.app/api/$1"
    }
  ]
}
